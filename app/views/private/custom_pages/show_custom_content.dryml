<page merge-attrs="&@custom_attributes">
  <prepend-container:>
<!--    <custom-page-top-menu if="&@custom_page.show_top_menu?" />  -->
  </prepend-container:>
  <aside:>
      <custom-page-aside  />
  </aside:>
  <content-header:>
    <div class="static-page-header">
