<index-page>
  <content: replace>
    <if test="&current_user.logged_in?">
    <%= link_to "new event", private_new_event_path, :class => "new-event-button btn btn-primary" %>
    </if>
    <div class="calendar-holder">
      <div id='calendar'></div>
    </div>
    <script>
      $(document).ready(function() {
      $('#calendar').windowResize();
      })
    </script>
  </content:>
</index-page>
