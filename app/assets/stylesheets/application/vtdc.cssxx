}


.main-dddnav a:hover {
	width: 100px;
    height: 23px;
	color: #5C722A;
	text-decoration: underline;
	background: transparent;
}
.page-header .navigation.main-nav a:hover, .page-header .navigation.main-nav li.current a:hover{
	color: #ffffff;
	background: url(menu_hov.png) no-repeat;
}

.page-header .navigation.main-nav li.current a {
    background: transparent;
    border: none;
    color: #222222;
}

