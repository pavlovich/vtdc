	display: inline; 
	
}
.content {
    background: none repeat scroll 0 0 #98B74C;
    margin: 0 auto;
    text-align: left;
    width: 793px;
}

.section.with-flash.content{
	background: url("table_back.gif") repeat-x scroll center top #98B74C;
}

.page-header .navigation.main-nav a {
	float: left;
	width: 100px;
    display: block;
	text-align: center;
	text-decoration: none;
	color: #ffffff;
	font-weight: bold;
	paddlkjing-top: 10px;
