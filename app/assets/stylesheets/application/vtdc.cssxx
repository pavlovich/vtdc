.login-page, .forgot-password-page {
    margin: auto;
    width: 793px;
}

fieldset.feckless-fields .field{
    margin-right: 10px;
}

fieldset.feckless-fields.inline-vertical .field {
    clear: none;
    display: inline-block;
    float: none;
    height: 1%;
    margin-bottom: 4px;
    margin-top: 0;
    padding: 0;
}

.edit-page .content-header .delete-button {
    float: right;
    margin-top: -25px;
    margin-right: 10px;
}

*
{
margin: 0px;
padding: 0px;
}

a:link {
	color: #F5FFDC;
}

a:hover {
	text-decoration: underline;
	color: #FF0000;
}

a:visited {
	color: #F5FFDC;
}

body {
	
	background: #78952D url(back_all.gif) repeat-x;
	font: 13px Arial, Helvetica, sans-serif;
	color: #000000;
	width: 793px;
}

#content
{
	margin: 0 auto;
	background: #98B74C;
	width: 793px;
